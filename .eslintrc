{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended",
    "@vue/typescript/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions":{
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"},
  "plugins":[
    "vue", "@typescript-eslint"
  ],
  "rules": {
    // "no-undef": "off"
  }
}

